
plus(plus(plus(plus(plus(5,6),x),x),5),x) -> OK
plus(mul(mul(12,pow(x,3)),mul(16,pow(x,2))),mul(cos(x),sin(x)))
mul(sin(x),mul(cos(x),plus(mul(pow(x,3),plus(x,5)),mul(4,pow(x,2)))))


